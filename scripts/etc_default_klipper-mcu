# Configuration for /etc/init.d/klipper-mcu

KLIPPER_HOST_MCU_USER=root
KLIPPER_HOST_MCU_SERIAL=/tmp/klipper_host_mcu
KLIPPER_HOST_MCU_LOG=/$KLIPPER_HOST_MCU_USER/printer_data/logs/klipper-mcu.log
KLIPPER_HOST_MCU_EXEC=/usr/local/bin/klipper_mcu
KLIPPER_HOST_MCU_ARGS=" -r -I ${KLIPPER_HOST_MCU_SERIAL}"
