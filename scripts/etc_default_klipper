# Configuration for /etc/init.d/klipper

KLIPPY_USER=root
KLIPPY_USER_DIR=/root
KLIPPY_CONFIG=$KLIPPY_USER_DIR/printer_data/config/printer.cfg
KLIPPY_LOG=$KLIPPY_USER_DIR/printer_data/logs/klippy.log
KLIPPY_SOCKET=$KLIPPY_USER_DIR/printer_data/comms/klippy.sock
KLIPPY_PRINTER=/tmp/printer
KLIPPY_EXEC=$KLIPPY_USER_DIR/klippy-env/bin/python
KLIPPY_ARGS="$KLIPPY_USER_DIR/klipper/klippy/klippy.py $KLIPPY_CONFIG -l $KLIPPY_LOG -a $KLIPPY_SOCKET"
