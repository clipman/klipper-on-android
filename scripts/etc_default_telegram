#  Configuration for /etc/init.d/telegram

TELEGRAM_USER=root
TELEGRAM_USER_DIR=/root
TELEGRAM_CONFIG=$TELEGRAM_USER_DIR/printer_data/config/telegram.conf
TELEGRAM_LOG=$TELEGRAM_USER_DIR/printer_data/logs/telegram.log
TELEGRAM_EXEC=$TELEGRAM_USER/moonraker-telegram-bot-env/bin/python
TELEGRAM_ARGS="$TELEGRAM_USER_DIR/moonraker-telegram-bot/bot/main.py -c $TELEGRAM_CONFIG -l $TELEGRAM_LOG"
