# Configuration for /etc/init.d/moonraker

MOONRAKER_USER=root
MOONRAKER_CONFIG=/$MOONRAKER_USER/printer_data/config/moonraker.conf
MOONRAKER_LOG=/$MOONRAKER_USER/printer_data/logs/moonraker.log
MOONRAKER_PRINTER=/tmp/printer
MOONRAKER_EXEC=/$MOONRAKER_USER/moonraker-env/bin/python
MOONRAKER_ARGS="/$MOONRAKER_USER/moonraker/moonraker/moonraker.py -c $MOONRAKER_CONFIG -l $MOONRAKER_LOG"
